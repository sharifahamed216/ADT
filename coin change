#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    vector<int>coins(n);
    for(int i=0;i<n;i++){
        cin>>coins[i];
    }
    int s;
    cin>>s;
    vector<int>dp(s+1,0);
    dp[0]=1;
    for(int coin:coins){
        for(int i=coin;i<=s;i++){
            dp[i]=dp[i]+dp[i-coin];
        }
    }
    cout<<dp[n];
}
