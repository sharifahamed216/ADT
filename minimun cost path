#include<bits/stdc++.h>
using namespace std;
int main(){
    int n,m;
    cin>>n>>m;
    int arr[n][m];
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            cin>>arr[i][j];
        }
    }
    int dp[n][m];
    dp[0][0]=arr[0][0];
    for(int i=1;i<n;i++){
        dp[i][0]=arr[i][0]+dp[i-1][0];
    }
    for(int j=1;j<m;j++){
        dp[0][j]=arr[0][j]+dp[0][j-1];
    }
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            dp[i][j]=arr[i][j]+min(dp[i-1][j],dp[i][j-1]);
        }
    }
    cout<<dp[n-1][m-1];
}



//output:
3 3
1 2 3 
4 8 2 
1 5 3
