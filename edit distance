#include<bits/stdc++.h>
using namespace std;
int main(){
    string s1,s2;
    cin>>s1>>s2;
    int ins,del,upt;
    cin>>ins>>del>>upt;
    int n=s1.length();
    int m=s2.length();
    vector<vector<int>>dp(n+1,vector<int>(m+1,0));
    for(int i=0;i<n;i++){
        dp[i][0]=i*del;
        for(int j=0;j<m;j++){
            dp[0][j]=j*ins;
        }
    }
    for(int i=1;i<=n;i++){
        for(int j=1;j<=m;j++){
            if(s1[i-1]==s2[j-1]){
                dp[i][j]=dp[i-1][j-1];
            }else{
                dp[i][j]=1+min(dp[i-1][j],dp[i][j-1]);
                int d=dp[i-1][j]+del;
                int inst=dp[i][j-1]+ins;
                int s=dp[i-1][j-1]+upt;
                dp[i][j]=min({d,inst,s});
            }
        }
        
    }
    cout<<dp[n][m];
    
}
