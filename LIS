//LENGTH OF LONGEST INCREASING SEQUENCE 

// You are using GCC
#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main(){
    int n;
    cin>>n;
    vector<int>num(n);
    vector<int>dp(n,1);
    for(int i=0;i<n;i++){
        cin>>num[i];
    }
    if(n==0){
        cout<<"Length of the longest increasing subsequence: 0";
    }
    for(int i=1;i<n;i++){
        for(int j=0;j<i;j++){
            if(num[i]>num[j]){
                dp[i]=max(dp[i],dp[j]+1);
            }
        }
    }
    cout<<"Length of the longest increasing subsequence: "<<*max_element(dp.begin(),dp.end());
}
